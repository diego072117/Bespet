	<queryString>
		<![CDATA[SELECT
     asignarservicio.`id_Asignar` AS asignarservicio_id_Asignar,
     asignarservicio.`Estado` AS asignarservicio_Estado,
     mascota.`Nombre` AS mascota_Nombre,
     servicios.`Nombre` AS servicios_Nombre,
     peluquero.`Nombre` AS peluquero_Nombre
FROM
     `mascota` mascota INNER JOIN `asignarservicio` asignarservicio ON mascota.`id_Mascota` = asignarservicio.`id_Mascota`
     INNER JOIN `servicios` servicios ON asignarservicio.`id_Servicios` = servicios.`id_Servicios`
     INNER JOIN `peluquero` peluquero ON asignarservicio.`id_Peluquero` = peluquero.`id_Peluquero`  WHERE peluquero.Nombre = $P{peluquero};]]>
	</queryString>
